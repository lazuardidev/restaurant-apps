{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-restricted-globals": 0,
    "no-console": 0,
    "linebreak-style": 0,
    "no-tabs": 0,
    "no-underscore-dangle": 0
  }
}